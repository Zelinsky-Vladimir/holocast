generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("NX_DATABASE_URL")
  shadowDatabaseUrl = env("NX_SHADOW_DATABASE_URL")
}

model User {
  id        String   @id @unique @default(uuid())
  email     String   @unique
  password  String
  username  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
